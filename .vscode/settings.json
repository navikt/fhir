{
    "files.exclude": {
        "**/fsh-generated/": true,
        "**/input-cache/": true,
        "**/output/": true,
        "**/temp/": true,
        "**/template/": true,
        "**/*.jar": true
    },
    "json.schemas": [
        {
            "fileMatch": [
                "**/examples/**/*.json"
            ],
            "url": "https://www.hl7.org/fhir/fhir.schema.json"
        }
    ],
    "editor.tabSize": 2
}