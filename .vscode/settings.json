{
    "files.exclude": {
        "package-cache": true,
        "**/input-cache/": true,
        "**/temp/": true,
        "**/template/": true,
        "**/*.jar": true
    },
    "json.schemas": [
        {
            // The schema provides IntelliSense when editing json files containing fhir resources (e.g. StructureDefinition). 
            "fileMatch": [
                "igs/*/input/*/*.json",
            ],
            "url": "https://www.hl7.org/fhir/fhir.schema.json"
        }
    ],
    "editor.tabSize": 2,
    "files.eol": "\n"
}